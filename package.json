{
  "name": "vant-vue3-starter",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --config eslint.config.cjs --fix --ext .js,.vue",
    "format": "prettier --write \"**/*.{js,vue,json,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "@vueuse/core": "^10.11.1",
    "pinia": "^3.0.3",
    "vant": "^4.9.21",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css,scss}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.7",
    "eslint-plugin-prettier": "^4.2.3",
    "eslint-plugin-vue": "^10.3.0",
    "husky": "^9.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "tailwindcss": "^4.1.11",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^6.3.5",
    "vue-eslint-parser": "^10.2.0"
  }
}
